<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="item_container" maxHeight="16" minHeight="16" prefHeight="16" prefWidth="1.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane maxHeight="16" minHeight="16" minWidth="0.0" prefHeight="16" prefWidth="-1.0" snapToPixel="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <BorderPane prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER_LEFT">
                    <center>
                        <Label fx:id="title" alignment="CENTER_LEFT" contentDisplay="LEFT" ellipsisString="" focusTraversable="false" prefHeight="-1.0" prefWidth="-1.0" text="" textOverrun="CLIP" BorderPane.alignment="CENTER_LEFT">
                            <font>
                                <Font size="11.0" />
                            </font>
                        </Label>
                    </center>
                    <left>
                        <Label fx:id="artist" contentDisplay="LEFT" ellipsisString="" focusTraversable="false" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" text="" BorderPane.alignment="CENTER_LEFT">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                    </left>
                </BorderPane>
            </center>
            <right>
                <Label fx:id="duration" alignment="CENTER" contentDisplay="CENTER" ellipsisString="" maxWidth="-1.0" prefHeight="-1.0" wrapText="false" BorderPane.alignment="CENTER_RIGHT">
                    <font>
                        <Font size="10.0" />
                    </font>
                </Label>
            </right>
            <left>
                <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" BorderPane.alignment="CENTER">
                    <children>
                        <ImageView fx:id="live" fitHeight="12.0" fitWidth="0.0" opacity="1.0" pickOnBounds="true" preserveRatio="true" smooth="false" style="">
                            <image>
                                <Image url="@item_playing.png" />
                            </image>
                            <HBox.margin>
                                <Insets left="2.0" right="3.0" />
                            </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="icon" managed="false" pickOnBounds="true" preserveRatio="true" smooth="false" visible="false">
                            <image>
                                <Image url="@icon_youtube.png" />
                            </image>
                            <HBox.margin>
                                <Insets right="4.0" />
                            </HBox.margin>
                            <effect>
                                <ColorAdjust brightness="-0.74" saturation="-1.0" />
                            </effect>
                        </ImageView>
                    </children>
                </HBox>
            </left>
        </BorderPane>
    </children>
</AnchorPane>
